{
  "name": "heroku-angular2-seed",
  "version": "1.0.0",
  "description": "A simple starter Angular2 project ready to deploy to Heroku",
  "main": "server/app.ts",
  "scripts": {
    "build": "NODE_ENV=production node node_modules/.bin/webpack && cp src/index.html dist/index.html",
    "watch": "npm run build -- --watch",
    "postinstall": "npm run build",
    "client": "webpack-dev-server --inline --progress --port 3000 --content-base src",
    "server": "tsc -p ./server && node dist/server/app.js",
    "start": "concurrently \"npm run server\" \"npm run client\""
  },
  "contributors": [
    "Rob Wormald <robwormald@gmail.com>",
